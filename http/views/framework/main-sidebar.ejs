<aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="index3.html" class="brand-link">
                <img src="dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8" />
                <span class="brand-text font-weight-light">MI-PALOMA</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image" />
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Ricardo Benitez</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        {Main-Sidebar:[
                          {href:"pages/calendar.html",p:"Calendar", class:"nav-icon fas fa-calendar-alt"}
                          {href:"pages/gallery.html",p:"Gallery", class:"nav-icon far fa-image"}
                          {href:"pages/kanban.html",p:"Kanban Board", class:"nav-icon fas fa-columns"}
                        
                        ]}
                         with font-awesome or any other icon font library -->

                        <li class="nav-header"><%=Sidebar.header%></li>
                        <% for(var x in Sidebar.li) { %>
                          <li class="nav-item">
                              <a href="#<%= Sidebar.li[x].href %>" class="nav-link">
                                <i class="<%= Sidebar.li[x].class %>"></i>
                                <p><%= Sidebar.li[x].p %></p>
                              </a>
                          </li>
                        <% } %>  
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>